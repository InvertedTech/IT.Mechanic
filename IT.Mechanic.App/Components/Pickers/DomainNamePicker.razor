<MudSelect Value="@Domain" ValueChanged="DomainChanged">
    @foreach (var domainName in _domains)
    {
        <MudSelectItem Value="@domainName" />
    }
</MudSelect>

@code {
    [Parameter]
    public string Domain { get; set; }

    [Parameter]
    public EventCallback<string> DomainChanged  { get; set; }

    private List<string> _domains = ["example1.com", "example2.com", "example3.com"];
}
